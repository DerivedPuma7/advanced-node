{
   "name": "advanced-node",
   "version": "1.0.0",
   "description": "API desenvolvida ao longo do treinamento de NodeJS avan√ßado, oferecido pelo professor Rodrigo Manguinho",
   "main": "index.js",
   "scripts": {
      "start": "node -r dotenv/config dist/main",
      "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
      "build": "rimraf dist && tsc -p tsconfig-build.json",
      "prepare": "husky install",
      "test": "jest --passWithNoTests --no-cache --runInBand",
      "test:watch": "npm test -- --watch",
      "test:staged": "npm test -- --findRelatedTests",
      "test:coverage": "npm test -- --coverage",
      "test:integration": "npm test -- --config ./jest.integration.config.js",
      "test:fb-api": "npm run test:integration -- ./tests/external/facebook-api.test.ts",
      "test:aws-s3": "npm run test:integration -- ./tests/external/aws-s3-file-storage.test.ts"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/DerivedPuma7/advanced-node.git"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/DerivedPuma7/advanced-node/issues"
   },
   "homepage": "https://github.com/DerivedPuma7/advanced-node#readme",
   "devDependencies": {
      "@jest-mock/express": "^2.0.1",
      "@types/aws-sdk": "^2.7.0",
      "@types/axios": "^0.14.0",
      "@types/cors": "^2.8.13",
      "@types/express": "^4.17.17",
      "@types/jest": "^29.4.0",
      "@types/jsonwebtoken": "^9.0.1",
      "@types/module-alias": "^2.0.1",
      "@types/multer": "^1.4.7",
      "@types/node": "^18.14.6",
      "@types/supertest": "^2.0.12",
      "@types/uuid": "^9.0.1",
      "husky": "^8.0.3",
      "jest": "^29.5.0",
      "jest-mock-extended": "^3.0.3",
      "lint-staged": "^13.1.2",
      "pg-mem": "1.9.17",
      "supertest": "^6.3.3",
      "ts-jest": "^29.0.5",
      "ts-node-dev": "^2.0.0",
      "typescript": "^4.9.5"
   },
   "engines": {
      "node": "16.x"
   },
   "dependencies": {
      "aws-sdk": "^2.1344.0",
      "axios": "^1.3.4",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "jsonwebtoken": "^9.0.0",
      "module-alias": "^2.2.2",
      "multer": "^1.4.5-lts.1",
      "pg": "^8.10.0",
      "reflect-metadata": "^0.1.13",
      "rimraf": "^4.3.1",
      "typeorm": "0.2.29",
      "uuid": "^9.0.0"
   }
}
